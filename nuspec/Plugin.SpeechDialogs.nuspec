<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Plugin.SpeechDialogs</id>
        <version>1.0.0-beta1-appsolu1</version>
        <title>ACR Speech Dialogs</title>
